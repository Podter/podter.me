{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "podter-website",
  "main": "dist/_worker.js/index.js",
  "compatibility_date": "2024-09-25",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist",
    "not_found_handling": "404-page"
  },
  "observability": {
    "enabled": true
  },
  "d1_databases": [
    {
      "binding": "D1_DATABASE",
      "database_name": "podter-website",
      "database_id": "e7342b72-bb1e-4c9e-8467-7229a7afdf38",
      "migrations_dir": "migrations"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "KV_CACHE",
      "id": "46b5add58ec24b038a97ea73d1723f41"
    }
  ]
}
